            <td>{{ entity.email }}</td>
        </tr>
        <tr>
            <th colspan="8"></th>           
        </tr>
        <tr>
            <th>Commentaires :</th>
            <td colspan="5">{{ entity.commentaire }}</td>                
            <td>        
                <a href="{{ path('machine_edit', { 'id': entity.id }) }}">
                    Editer
                </a>
            </td>
            <td>                
                <a href="{{ path('machine_delete', { 'id': entity.id }) }}">
                    Supprimer
                </a>
            
            </td>            
    </tbody>
</table>

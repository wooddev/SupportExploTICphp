<?php

namespace Explotic\AdminBundle\Entity;

use FOS\UserBundle\Entity\User as BaseUser;
use Doctrine\ORM\Mapping as ORM;

/**
 * User
 */
class User extends BaseUser
{
    /**
     * @var integer
     */
    protected $id;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }
    
    public function __construct(){
        parent::__construct();
    }
    /**
     * @var \Explotic\TiersBundle\Entity\Entreprise
     */
    private $entreprise;

    /**
     * @var \Explotic\TiersBundle\Entity\Entreprise
     */
    private $recruteur;

    /**
     * @var \Explotic\TiersBundle\Entity\Entreprise
     */
    private $stagiaire;

    /**
     * @var \Explotic\TiersBundle\Entity\Entreprise
     */
    private $formateur;


    /**
     * Set entreprise
     *
     * @param \Explotic\TiersBundle\Entity\Entreprise $entreprise
     * @return User
     */
    public function setEntreprise(\Explotic\TiersBundle\Entity\Entreprise $entreprise = null)
    {
        $this->entreprise = $entreprise;
    
        return $this;
    }

    /**
     * Get entreprise
     *
     * @return \Explotic\TiersBundle\Entity\Entreprise 
     */
    public function getEntreprise()
    {
        return $this->entreprise;
    }

    /**
     * Set recruteur
     *
     * @param \Explotic\TiersBundle\Entity\Entreprise $recruteur
     * @return User
     */
    public function setRecruteur(\Explotic\TiersBundle\Entity\Entreprise $recruteur = null)
    {
        $this->recruteur = $recruteur;
    
        return $this;
    }

    /**
     * Get recruteur
     *
     * @return \Explotic\TiersBundle\Entity\Entreprise 
     */
    public function getRecruteur()
    {
        return $this->recruteur;
    }

    /**
     * Set stagiaire
     *
     * @param \Explotic\TiersBundle\Entity\Entreprise $stagiaire
     * @return User
     */
    public function setStagiaire(\Explotic\TiersBundle\Entity\Entreprise $stagiaire = null)
    {
        $this->stagiaire = $stagiaire;
    
        return $this;
    }

    /**
     * Get stagiaire
     *
     * @return \Explotic\TiersBundle\Entity\Entreprise 
     */
    public function getStagiaire()
    {
        return $this->stagiaire;
    }

    /**
     * Set formateur
     *
     * @param \Explotic\TiersBundle\Entity\Entreprise $formateur
     * @return User
     */
    public function setFormateur(\Explotic\TiersBundle\Entity\Entreprise $formateur = null)
    {
        $this->formateur = $formateur;
    
        return $this;
    }

    /**
     * Get formateur
     *
     * @return \Explotic\TiersBundle\Entity\Entreprise 
     */
    public function getFormateur()
    {
        return $this->formateur;
    }
}
<h1>InterventionEntreprise</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Nom</th>
            <td>{{ entity.nom }}</td>
        </tr>
        <tr>
            <th>Dureejour</th>
            <td>{{ entity.dureeJour }}</td>
        </tr>
        <tr>
            <th>Stade</th>
            <td>{{ entity.stade }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('interventionentreprise') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('interventionentreprise_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('interventionentreprise_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>

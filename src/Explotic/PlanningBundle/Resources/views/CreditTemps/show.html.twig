<h1>CreditTemps</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Tempstotal</th>
            <td>{{ entity.tempsTotal }}</td>
        </tr>
        <tr>
            <th>Tempsreserve</th>
            <td>{{ entity.tempsReserve }}</td>
        </tr>
        <tr>
            <th>Tempsconsomme</th>
            <td>{{ entity.tempsConsomme }}</td>
        </tr>
        <tr>
            <th>Tempsdisponible</th>
            <td>{{ entity.tempsDisponible }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('credittemps') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('credittemps_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('credittemps_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>

<h1>Reservation</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Dateheuredebut</th>
            <td>{{ entity.dateHeureDebut|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Dateheurefin</th>
            <td>{{ entity.dateHeureFin|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Jour</th>
            <td>{{ entity.jour }}</td>
        </tr>
        <tr>
            <th>Semaine</th>
            <td>{{ entity.semaine }}</td>
        </tr>
        <tr>
            <th>Annee</th>
            <td>{{ entity.annee }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('reservation') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('reservation_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('reservation_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>

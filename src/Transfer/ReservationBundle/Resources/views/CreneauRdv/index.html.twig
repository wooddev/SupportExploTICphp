{%extends '::base.html.twig'%}
{%block body%}
<h1>CreneauRdv list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Jour</th>
            <th>Heure</th>
            <th>Minute</th>
            <th>Duree</th>
            <!--th>Heuredebut</th>
            <th>Heurefin</th-->
            <th>Disponibilite</th>
            <th>Semaine</th>
            <th>Ann√©e</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('creneaurdv_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.jour }}</td>
            <td>{{ entity.heure }}</td>
            <td>{{ entity.minute }}</td>
            <td>{{ entity.duree }}</td>
            {#<td>{{ entity.heureDebut }}</td>
            <td>{{ entity.heureFin }}</td>#}
            <td>{{ entity.disponibilite }}</td>
            <td>{{ entity.semaine }}</td>
            <td>{{ entity.annee }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('creneaurdv_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('creneaurdv_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('creneaurdv_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
{%endblock%}
{% extends '::base.html.twig' %}

{% block body -%}
    <h1>AgentReception list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Prenom</th>
                <th>Courriel</th>
                <th>Telephone</th>
                <th>Dateenregistrement</th>
                <th>Datemodification</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('agentreception_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.nom }}</td>
                <td>{{ entity.prenom }}</td>
                <td>{{ entity.courriel }}</td>
                <td>{{ entity.telephone }}</td>
                <td>{% if entity.dateEnregistrement %}{{ entity.dateEnregistrement|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.dateModification %}{{ entity.dateModification|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('agentreception_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('agentreception_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('agentreception_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
